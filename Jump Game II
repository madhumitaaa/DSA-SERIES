https://leetcode.com/problems/jump-game-ii/submissions/1838642309/
class Solution {
public:
    int jump(vector<int>& nums) {
        int n=nums.size();
       int i=0;
       int end=0;
       int farthest=0;
         int cnt=0;
        for(i=0;i<=n-2;i++)
        {
            farthest=max(farthest,i+nums[i]);
            if(i==end){
                cnt++;
                end=farthest;
            }
            }
return cnt;
  
    }
};
