https://leetcode.com/problems/remove-duplicates-from-sorted-array-ii/submissions/1828253873/?envType=study-plan-v2&envId=top-interview-150

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
             int n=nums.size();
             if(n<=2)return n;
           int j=2;
           for(int i=2;i<n;i++)
           {
                if(nums[i]!=nums[j-2])
                {
                    nums[j++]=nums[i];
                }
           }

           return j;

    }
};
