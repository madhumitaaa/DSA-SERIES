https://leetcode.com/problems/sqrtx/submissions/1788593617/

class Solution {
public:
    int mySqrt(int x) {
        int h=x/2;
        int low=1,high=h;
        int ans=0;
        if(x==0||x==1)return x;
        while(low<=high)
        {
            long long  mid=low+(high-low)/2;
            if(mid*mid==x)return mid;
            else if(mid*mid<x){
                ans=mid;
                low=mid+1;
            }
             else high=mid-1;
        }
        return ans;
    }
};
