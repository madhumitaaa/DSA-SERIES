https://www.geeksforgeeks.org/problems/reverse-a-string/1

class Solution {
  public:
    string reverseString(string& s) {
        // code here
        int n=s.size()-1;
        int l=0;
        int r=n;
        while(l<r)
        {
            swap(s[l],s[r]);
            l++;
            r--;
        }
        return s;
    }
};
