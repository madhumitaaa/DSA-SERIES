https://www.geeksforgeeks.org/problems/reverse-a-string-using-stack/1


class Solution {
  public:
    string reverse(const string& S) {
        // code here
        int len=S.length();
        stack<char>s;
        for( char ch:S)
        {
            s.push(ch);
        }
         
         
         string reversed;
         while(!s.empty())
         {
             reversed+=s.top();
             s.pop();
         }
         
         return reversed;
         
         
         
         
         
         
         
         
         
         
    }
};
