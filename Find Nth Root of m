https://www.geeksforgeeks.org/problems/find-nth-root-of-m5843/1


#include<bits/stdc++.h>

class Solution {
  public:
    int nthRoot(int n, int m) {
        // Code here
      
         int left=1;
            int right=m;
        while(left<=right){
            long long mid=left+(right-left)/2;
            long long ans=1;
            for(int i=1;i<=n;i++)
            {
                ans*=mid;
                if(ans>m)break;
            }
            if(ans==m)return mid;
             else if(ans<m)left=mid+1;
             else right=mid-1;
        }
          return -1;
          
    }
};
