https://www.geeksforgeeks.org/problems/attend-all-meetings/1

class Solution {
  public:
    bool canAttend(vector<vector<int>> &arr) {
        // Code Here
        sort(arr.begin(),arr.end());
        int n=arr.size();
        for(int i=1;i<n;i++)
        {
            if(arr[i-1][1]>arr[i][0]){
                return false;
            }
        }
        return true;
    }
};
