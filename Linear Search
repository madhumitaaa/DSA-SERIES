https://takeuforward.org/plus/dsa/problems/linear-search

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n=nums.size();
        int pos=0;
        for(int i=0;i<n;i++)
        {
            if(nums[i]!=0){
                nums[pos++]=nums[i];
            }
        }

        while(pos<n){
            nums[pos++]=0;
        }
    }
};
