https://leetcode.com/problems/maximum-nesting-depth-of-the-parentheses/


class Solution {
public:
    int maxDepth(string s) {
        int open=0,maxi=0;
        for(char ch:s)
        {
            if(ch=='('){
                open++;
                 maxi=max(maxi,open);
            }
             else if(ch==')') {
                open--;
             }
              else continue;
             
        }
return maxi;
       
    }
 
};
