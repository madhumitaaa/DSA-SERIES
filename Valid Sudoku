https://leetcode.com/problems/valid-sudoku/description/?envType=problem-list-v2&envId=array

class Solution {
public:
 
    bool isValidSudoku(vector<vector<char>>& board) {
        int n=9;
      
        bool row[9][10]={false};
         bool col[9][10]={false};
         bool box[9][10]={false};
        for(int r=0;r<n;r++)
        {
            for(int c=0;c<n;c++)
            {
                if(board[r][c]=='.')continue;
                int num=board[r][c]-'0';
                int b=(r/3)*3+(c/3);
                if(row[r][num]||col[c][num]||box[b][num])return false;
                row[r][num]=true;
                col[c][num]=true;
                box[b][num]=true;
            }
        }
        return true;

    }
};
