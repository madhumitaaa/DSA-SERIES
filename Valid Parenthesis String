https://leetcode.com/problems/valid-parenthesis-string/

class Solution {
public:
    bool checkValidString(string s) {
        int n=s.length(),low=0,high=0;
        for(int i=0;i<n;i++)
        {
            char c=s[i];
            if(c=='('){low++;high++;}
            if(c==')'){low--;high--;}
            if(c=='*'){low--;high++;}
              low=max(low,0);
          if(high<0)return false;
         
        }
         
            
        return low==0;
    }
};
