https://leetcode.com/problems/subarray-sum-equals-k/description/

class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        unordered_map<int,int>mpp;
        int prefix=0,cnt=0;
        mpp[0]=1;
        for(int num:nums)
        {
            prefix+=num;
            if(mpp.find(prefix-k)!=mpp.end())
            {
                cnt+=mpp[prefix-k];
            }
        mpp[prefix]++;
        }
        return cnt;
    }
};
