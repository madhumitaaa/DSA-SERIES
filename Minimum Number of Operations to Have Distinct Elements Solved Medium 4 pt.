https://leetcode.com/contest/biweekly-contest-172/problems/minimum-number-of-operations-to-have-distinct-elements/description/

class Solution {
public:
    int minOperations(vector<int>& nums) {
        int n=nums.size();
        unordered_set<int>seen;
        int l=0;
        for(int i=n-1;i>=0;i--)
            {
                if(seen.count(nums[i])){
                    l=i+1;
                    break;
                }
                seen.insert(nums[i]);
            }
        return (l+2)/3;
    }
};
