https://leetcode.com/problems/broken-calculator/submissions/1863093750/

class Solution {
public:
    int brokenCalc(int startValue, int target) {
        int ops=0;
        while(target>startValue)
        {
            if(target%2==0)
            {
                target/=2;
                
            }
            else{
                target+=1;
                  
            }
              ops++;
          
        }
        return ops+(startValue-target);
    }
};
