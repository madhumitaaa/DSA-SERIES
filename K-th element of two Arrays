https://www.geeksforgeeks.org/problems/k-th-element-of-two-sorted-array1317/1

class Solution {
  public:
    int kthElement(vector<int> &a, vector<int> &b, int k) {
        // code here
        vector<int>temp;
        for(int i=0;i<a.size();i++)
        {
            temp.push_back(a[i]);
        }
         for(int i=0;i<b.size();i++)
        {
            temp.push_back(b[i]);
        }
        sort(temp.begin(),temp.end());
        return temp[k-1];
    }
};
