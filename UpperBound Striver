https://takeuforward.org/plus/dsa/problems/upper-bound

class Solution{
public:
    int upperBound(vector<int> &nums, int x){
        int n=nums.size();
        int left=0,right=n-1;
        int ans=n;
        while(left<=right)
        {
            int mid=left+(right-left)/2;
            if(nums[mid]>x){
                ans=mid;
                right=mid-1;
            }
             else{
                left=mid+1;
             }
        }
        return ans;
    }
};
